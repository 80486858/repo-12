{
  "name": "@kiwicom/orbit-tracking",
  "version": "0.0.1",
  "description": "Tracking for orbit design system",
  "author": "Kiwi.com",
  "license": "MIT",
  "private": true,
  "main": "dist/cli.js",
  "module": "dist/index.js",
  "bin": "./bin/tracking.js",
  "type": "module",
  "files": [
    "bin",
    "dist",
    "src/**/!(*.test).@(ts|json)"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kiwicom/orbit",
    "directory": "packages/orbit-tracking"
  },
  "scripts": {
    "dev": "tsc --w",
    "build": "yarn clean && tsc && yarn patch",
    "patch": "npx tsc-esm-fix --target='dist' --ext='.js' --dirnameVar=false --filenameVar=false",
    "clean": "npx rimraf dist",
    "types": "tsc --noEmit",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "postinstall": "yarn build"
  },
  "keywords": [
    "cli",
    "orbit",
    "tracking",
    "design-system",
    "react-scanner"
  ],
  "devDependencies": {
    "@types/dotenv-safe": "^8.1.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.38",
    "@types/sade": "^1.7.4",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "dotenv-safe": "^8.2.0",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "ora": "^6.1.0",
    "react-scanner-orbit": "^1.3.3",
    "sade": "^1.8.1"
  }
}
