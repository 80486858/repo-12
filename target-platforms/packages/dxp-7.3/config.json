{
	"*": {
		".babelrc": {
			"presets": ["liferay-standard"]
		},
		"copy-plugins": ["exclude-imports"],
		"plugins": ["replace-browser-modules"],
		"post-plugins": [
			"namespace-packages",
			"inject-imports-dependencies",
			"inject-peer-dependencies"
		]
	},
	"/": {
		".babelrc": {
			"presets": ["liferay-standard"]
		},
		"plugins": ["resolve-linked-dependencies"],
		"post-plugins": ["namespace-packages", "inject-imports-dependencies"]
	},
	"config": {
		"imports": {
			"app-builder-web": {
				"/": "*"
			},
			"asset-taglib": {
				"/": "*"
			},
			"commerce-frontend-js": {
				"/": "*"
			},
			"data-engine-taglib": {
				"/": "*"
			},
			"dynamic-data-mapping-form-builder": {
				"/": "*"
			},
			"dynamic-data-mapping-form-field-type": {
				"/": "*"
			},
			"dynamic-data-mapping-form-renderer": {
				"/": "*"
			},
			"frontend-editor-ckeditor-web": {
				"/": "*"
			},
			"frontend-js-metal-web": {
				"metal": "*",
				"metal-component": "*",
				"metal-soy": "*",
				"metal-state": "*",
				"metal-dom": "*",
				"incremental-dom": "*",
				"incremental-dom-string": "*",
				"metal-ajax": "*",
				"metal-anim": "*",
				"metal-debounce": "*",
				"metal-drag-drop": "*",
				"metal-events": "*",
				"metal-incremental-dom": "*",
				"metal-path-parser": "*",
				"metal-position": "*",
				"metal-promise": "*",
				"metal-soy-bundle": "*",
				"metal-structs": "*",
				"metal-uri": "*",
				"metal-useragent": "*",
				"metal-web-component": "*",
				"xss-filters": "*",
				"metal-jsx": "*"
			},
			"frontend-js-react-web": {
				"classnames": "*",
				"prop-types": "*",
				"react": "*",
				"react-dom": "*",
				"/": "*"
			},
			"frontend-js-recharts": {
				"recharts": "*"
			},
			"frontend-js-spa-web": {
				"senna": "*"
			},
			"frontend-js-web": {
				"/": "*"
			},
			"frontend-taglib-chart": {
				"billboard.js": "*",
				"d3": "*",
				"d3-array": "*",
				"d3-axis": "*",
				"d3-brush": "*",
				"d3-chord": "*",
				"d3-collection": "*",
				"d3-color": "*",
				"d3-contour": "*",
				"d3-dispatch": "*",
				"d3-drag": "*",
				"d3-dsv": "*",
				"d3-ease": "*",
				"d3-fetch": "*",
				"d3-force": "*",
				"d3-format": "*",
				"d3-geo": "*",
				"d3-hierarchy": "*",
				"d3-interpolate": "*",
				"d3-path": "*",
				"d3-polygon": "*",
				"d3-quadtree": "*",
				"d3-random": "*",
				"d3-scale-chromatic": "*",
				"d3-scale": "*",
				"d3-selection": "*",
				"d3-shape": "*",
				"d3-time-format": "*",
				"d3-time": "*",
				"d3-timer": "*",
				"d3-transition": "*",
				"d3-voronoi": "*",
				"d3-zoom": "*"
			},
			"frontend-taglib-clay": {
				"clay-autocomplete": "*",
				"clay-button": "*",
				"clay-data-provider": "*",
				"clay-icon": "*",
				"clay-label": "*",
				"clay-link": "*",
				"clay-multi-select": "*",
				"clay-portal": "*",
				"clay-radio": "*",
				"@clayui/icon": "*",
				"@clayui/alert": "*",
				"@clayui/button": "*",
				"@clayui/form": "*",
				"@clayui/link": "*",
				"@clayui/modal": "*",
				"@clayui/shared": "*",
				"clay": "*",
				"clay-alert": "*",
				"clay-component": "*"
			},
			"item-selector-taglib": {
				"/": "*"
			},
			"map-common": {
				"/": "*"
			},
			"map-google-maps": {
				"/": "*"
			},
			"map-openstreetmap": {
				"/": "*"
			}
		}
	},
	"create-jar": {
		"output-dir": "dist"
	},
	"rules": [
		{
			"test": "\\.css$",
			"use": ["css-loader"]
		},
		{
			"exclude": "node_modules",
			"test": ".*/[^_][^/]+\\.scss$",
			"use": [
				{
					"loader": "css-loader",
					"options": {
						"emitCssFile": false,
						"extension": ".css"
					}
				}
			]
		}
	],
	"sources": ["src"]
}
